<UserControl x:Class="ufficio.Componenti.Dashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ufficio.Componenti"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <Button x:Name="btnUtenti" Content="UTENTI" HorizontalAlignment="Left" Margin="10,10,0,0" Width="100" Click="btnUtenti_Click" Height="97" VerticalAlignment="Top"/>
        <Button x:Name="btnPosizioni" Content="POSIZIONI" HorizontalAlignment="Left" Margin="128,10,0,0" Width="100" Height="97" VerticalAlignment="Top" Click="btnPosizioni_Click"/>
        <Button x:Name="btnImpostazioni" Content="IMPOSTAZIONI" HorizontalAlignment="Left" Margin="250,10,0,0" Width="100" Height="97" VerticalAlignment="Top" Click="btnImpostazioni_Click"/>
        <Button x:Name="btnLogout" Content="logout" Margin="0,10,11,0" Click="btnLogout_Click" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Width="39"/>

    </Grid>
</UserControl>
